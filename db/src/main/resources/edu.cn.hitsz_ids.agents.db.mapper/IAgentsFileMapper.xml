<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.cn.hitsz_ids.agents.db.mapper.IAgentsFileMapper">
    <select id="queryBridge" resultType="java.lang.String">
        SELECT `bridge`
        FROM `agents_file`
        WHERE `identity` = #{identity}
    </select>

    <select id="queryPathInfo" resultType="edu.cn.hitsz_ids.agents.db.pojo.returns.SearchPathReturns">
        SELECT `path`, `name`, 'directory', `bridge`
        FROM `agents_file`
        WHERE `identity` = #{identity}
    </select>
</mapper>